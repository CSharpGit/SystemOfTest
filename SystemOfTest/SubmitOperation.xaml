<Window x:Class="SystemOfTest.SubmitOperation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SystemOfTest"
        mc:Ignorable="d"
        Title="SubmitOperation" Height="120" Width="410" WindowStyle="None" Loaded="Window_Loaded" WindowStartupLocation="CenterScreen">
    <Grid>
    <Grid  HorizontalAlignment="Left" VerticalAlignment="Top" Width="400" Height="80" >
            <StackPanel Name="stackpanel1" RenderTransformOrigin="0.495,-5.075">
            </StackPanel>
        </Grid>
        <Grid HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,80,0,0" Width="400" Height="30">
            <Button HorizontalAlignment="Left" Content="结束考试" Style="{StaticResource btn-warning}" VerticalAlignment="Top" Margin="310,0,0,0" Width="80" Height="28" Click="Button_Click"></Button>
        </Grid>
    </Grid>
</Window>
